<div id="<%= dom_id editor %>">
  <% if action_name != "show" %>
    <%= link_to "Show this editor", editor, class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%= link_to 'Edit this editor', edit_editor_path(editor), class: "rounded-lg py-3 ml-2 px-5 bg-gray-100 inline-block font-medium" %>
    <hr class="mt-6">
  <% end %>
</div>
